<?xml version="1.0"?>
<robot name="custom_robot">
  <link name="base_link" />

  <joint name="base_joint" type="fixed">
    <parent link="base_link"/>
    <child link="link1"/>
  </joint>

  <joint name="joint1" type="continuous">
    <parent link="base_link"/>
    <child link="link1"/>
    <origin xyz="0 0 0.5" rpy="0 0 0" />
    <axis xyz="0 1 0" />
  </joint>

  <link name="link1" />
</robot>




